<form class="modal fade auto-submit" action="{% block action %}/{% endblock %}" id="{% block id %}modal{% endblock %}" tabindex="-1" role="dialog" enctype="multipart/form-data">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title">{% block title %}Modal title{% endblock %}</h5>
				<button type="button" class="close btn btn-round" data-dismiss="modal">
					<i class="material-icons">close</i>
				</button>
			</div>
			{% block tabs %}
			{% endblock %}
			<div class="modal-body">
				{% block body %}Modal body{% endblock %}
			</div>
			<div class="modal-footer">
				<button role="button" class="btn btn-primary" type="submit">
					{% block submitLabel %}Submit{% endblock %}
				</button>
			</div>
		</div>
	</div>
</form>